<%= link_to "My練習場一覧", mydriving_ranges_path , :style=>"color:black;"%>
<div>
  <% current_user.likes.each do |like| %>
    <% if like.place.variation == 'driving_range' %>
      <%= link_to place_path(like.place.id) do%>
        <table class="table">
          <tr>
            <th><%= like.place.name %></th>
            <th><%= like.place.access %></th>
            <iframe src="https://maps.google.co.jp/maps?output=embed&q=<%= like.place.address %>"></iframe>
          </tr>
        </table>
      <% end %>
    <% end %>
  <% end %>
</div>
