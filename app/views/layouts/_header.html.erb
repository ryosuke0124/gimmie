<body>
     <header class="navbar navbar-inverse navbar-position">
  <div class="container">
  <nav class='nav navbar-nav navbar-left' >
    <h2><%= link_to " gimmie ", root_path,:style=>"color:white;"%></h2>
      <% if user_signed_in? %>
        <span class="text-right text-muted" style="color:white"><%= current_user.name %> さんようこそ</span>
      <% end %>
  </nav>
  <nav>
    <ul class="nav navbar-nav navbar-right navbar-original">
      <li class="header-li"><%= link_to " Home", '/' ,:style=>"color:white;"%></li>
      <li class="header-li"><%= link_to " About", abouts_path,:style=>"color:white;" %></li>
      <% if user_signed_in? %>
        <li class="header-li"><%= link_to " Logout", destroy_user_session_path, method: :delete ,:style=>"color:white;" %></li>
        <li class="header-li"><%= link_to " MYpage", user_path(current_user) ,:style=>"color:white;" %></li>
        <li class="header-li"><%= link_to " Users", users_path(current_user) ,:style=>"color:white;" %></li>
      <% else %>
        <li class="header-li"><%= link_to " Login", new_user_session_path, :style=>"color:white;" %></li>
      <% end %>
      <li style="padding-top: 15px; padding-left: 15px;">
      <%#= search_form_for @search, url:  do |f| %>
      <%#= f.text_field :item_name_cont, placeholder: '利用シーンを選択...' %>
      <%#= f.submit ("検索する") %>
      <%# end %>
      </li>
    </ul>
  </nav>
  </div>
</header>
</body>